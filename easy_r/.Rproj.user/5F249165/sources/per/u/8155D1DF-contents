df_midterm <- data.frame(english = c(90, 80, 60, 70),
                         math = c(50, 60, 100, 70),
                         class = c(1,1,2,2))
df_midterm
write.csv(df_midterm, file = "df_midterm.csv")
saveRDS(df_midterm, file = "df_midterm.rds")
df_midterm
df_midterm <-readRDS("df_midterm.rds")
df_midterm
